<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treningsapp.ui.RegisterController">
    <children>
        <fx:include fx:id="actionScreen" source="AddSession.fxml" />
        <Pane fx:id="loginScreen" prefHeight="800.0" prefWidth="600.0" style="-fx-background-color: rgb(188, 255, 143);">
            <children>
                <TextField fx:id="txtUsername" alignment="CENTER" layoutX="189.0" layoutY="260.0" prefHeight="46.0" prefWidth="230.0" promptText="Username">
                    <font>
                        <Font name="Avenir Next Bold" size="17.0" />
                    </font>
                </TextField>
                <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="186.0" layoutY="313.0" prefHeight="47.0" prefWidth="230.0" promptText="Password">
                    <font>
                        <Font name="Avenir Next Bold" size="17.0" />
                    </font>
                </PasswordField>
                <Label alignment="CENTER" layoutX="1.0" layoutY="185.0" prefHeight="66.0" prefWidth="600.0" text="Login" textAlignment="CENTER" textFill="#4a4a4a">
                    <font>
                        <Font name="Avenir Next Bold" size="45.0" />
                    </font>
                </Label>
                <Button fx:id="btnLogin" layoutX="186.0" layoutY="377.0" mnemonicParsing="false" onAction="#loginFromUi" prefHeight="47.0" prefWidth="230.0" text="Login">
                    <font>
                        <Font name="Avenir Next Bold" size="14.0" />
                    </font>
                </Button>
                <Label fx:id="lblWrongLogin" alignment="CENTER" layoutX="37.0" layoutY="529.0" prefHeight="76.0" prefWidth="533.0">
                    <font>
                        <Font name="Avenir Next Bold" size="16.0" />
                    </font>
                </Label>
                <Button fx:id="btnSignup" layoutX="186.0" layoutY="467.0" mnemonicParsing="false" onAction="#signUpFromUi" prefHeight="46.0" prefWidth="230.0" text="Sign up">
                    <font>
                        <Font name="Avenir Next Bold" size="14.0" />
                    </font>
                </Button>
                <Label alignment="CENTER" layoutX="1.0" layoutY="424.0" prefHeight="37.0" prefWidth="600.0" text="or">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</Pane>
